program Project1;

uses
  System.StartUpCopy,
  FMX.Forms,
  ufrmPrincipal in 'ufrmPrincipal.pas' {frmPrincipal},
  ufrmCadastroPadrao in 'ufrmCadastroPadrao.pas' {frmCadastroPadrao};

